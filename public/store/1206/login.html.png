    sensors.registerPage({
        platform_type:'pc',
        site_type: 'shein',
        site_id: 'asia',
        site_language: 'en',
        currency: 'TRY',
        browser_language: navigator.language || navigator.userLanguage,
        local_time: function() {
            return new Date()
