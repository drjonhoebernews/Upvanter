        x.async = 1;
        x.src = p;
        w[n].para = para;
        y.parentNode.insertBefore(x, y);
      }
    })({
        sdk_url: '//sheinsz.ltwebstatic.com/she_dist/libs/sensors/shein.min.js',
        name: 'sensors',
        server_url: getServerUrl(),
        send_type: 'beacon',
        heatmap: {
            clickmap: 'default',
            scroll_notice_map: 'default',
            scroll_delay_time: 1000,
            collect_input: false
        },
        scrollmap: {
            collect_url: function() {
                // 只采集首页
                return window.SaPageInfo && window.SaPageInfo.page_name == 'page_home'
            }
        },
        show_log: getShowLog(),
        preset_properties: {
            url: true
        },
        batch_send: false,
