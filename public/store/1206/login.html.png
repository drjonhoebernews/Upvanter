<script>
    // SensorPageInfo.pageNm 在其他地方会重新赋值
    var SensorPageInfo = {
        pageNm: '',
        gameId: undefined,
    }
    function getServerUrl() {
        var SENSORS_REPORT_URL = 'https://api-sensors.shein.com'
        var environment = 'production';
        var url = '';
        switch(environment) {
            case 'gray':
                url = SENSORS_REPORT_URL + '/sa?project=project_gray'
                break
            case 'production':
                url = SENSORS_REPORT_URL + '/sa?project=production'
                break
            default:
                url = SENSORS_REPORT_URL + '/sa?project=default'
                break
        }
        return url
    }
    function getCookie(name) {
        var value = "; " + document.cookie;
        var parts = value.split("; " + name + "=");
        if (parts.length >= 2) {
            return decodeURIComponent(parts.pop().split(";").shift())
        } else {
            return undefined;
        }
    }
    function getShowLog() {
        // sensor-analysisi-show-log  2 展示 3 隐藏
        // 0 根据环境变量判断
        var tag = 0
        var result = 0
        try {
            tag = sessionStorage.getItem('sensor-analysisi-show-log') || getCookie('sensor-show-log') || 0
        } catch(e) {}
        switch (Number(tag)) {
            case 2:
                result = true
                break
            case 3:
                result = false
                break
            case 0:
                // result = 'production' === 'production' ? false : true
                resule = false
                break
        }
        return result
    }
    // 神策埋点
    (function(para) {
      var p = para.sdk_url, n = para.name, w = window, d = document, s = 'script',x = null,y = null;
